_starter () {
    echo "Downloading source code"
    git clone https://github.com/Sh6Kuro/PyroSupra-Userbot
    cd PyroSupra-Userbot/ && rm -rf .git
    python3 ../updater.py ../requirements.txt requirements.txt
    echo "PyroSupra-Userbot v0.2.0, Copyright Â© 2023 (jungleridamanvp) <https://github.com/Sh6Kuro>"
    python3 -m Sh6Project
}

_starter
